	"JS Vanilla Add Click Events on classes": {
		"prefix": "z_vanilla_click_on_classes",
		"body": [
		  "let ${1:variable_name} = document.querySelectorAll('${3:.classname}');",
		  "Array.from(${1:variable_name ${2:variable_name}}).forEach(link => {",
		  "    link.addEventListener('click', function(event) {",
		  "        console.log('Test');",
		  "    });",
		  "});"
		],
		"description": "JS Vanilla Add Click Events on classes"
	},


